#include<stdio.h>
int main()
{
    int marks;
    printf("enter the marks of student\n");
    scanf("%d",&marks);
    if(marks>=85)
    printf("\n\n Your Grade : A");

   else if(marks>=70)
     printf("\n\n Your Grade : B");

     else if(marks>=55)
       printf("\n\n Your Grade : C");

        else if(marks>=40)
          printf("\n\n Your Grade :D ");

   else
     printf("\n\n You Grade :F");
     return 0;
   }
